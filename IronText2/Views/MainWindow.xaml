<Window x:Class="IronText2.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:prism="http://prismlibrary.com/"
        prism:ViewModelLocator.AutoWireViewModel="True"
        Title="{Binding Title}" Height="350" Width="525">
    <DockPanel LastChildFill="True">
        <ContentControl DockPanel.Dock="Top" prism:RegionManager.RegionName="Toolbar"></ContentControl>
        <ItemsControl DockPanel.Dock="Left" prism:RegionManager.RegionName="Navigation"></ItemsControl>
        <TabControl prism:RegionManager.RegionName="Tabs"></TabControl>
    </DockPanel>

</Window>
